<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
            android:layout_width="81dp"
            android:layout_height="50dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/SekID" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.885"
            android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/textView2" android:text="0"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Minutt"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.567"
            android:id="@+id/textView" android:textSize="24sp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Timar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.567"
            android:id="@+id/textView4" android:textSize="24sp" app:layout_constraintHorizontal_bias="0.13"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sekund"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.567"
            android:id="@+id/textView2" android:textSize="24sp" app:layout_constraintHorizontal_bias="0.866"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kor mange spelarar er dykk?"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.077"
            android:id="@+id/textView3" android:textSize="24sp"/>
    <SeekBar
            style="@style/Widget.AppCompat.SeekBar.Discrete"
            android:layout_width="270dp"
            android:layout_height="105dp"
            android:max="6"
            android:progress="1"
            android:id="@+id/seekBarID" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="264dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="Spel"
            android:layout_width="159dp"
            android:layout_height="82dp"
            android:id="@+id/buttonID" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="24dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.502"/>
    <TextView
            android:text="3"
            android:layout_width="113dp"
            android:layout_height="36dp"
            android:id="@+id/textViewID" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="48dp" app:layout_constraintBottom_toTopOf="@+id/buttonID"
            app:layout_constraintHorizontal_bias="0.501" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/seekBarID" app:layout_constraintVertical_bias="0.0"
            android:textAlignment="center" android:textSize="24sp"/>
    <EditText
            android:layout_width="81dp"
            android:layout_height="50dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/TimID" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.083"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" android:text="0"/>
    <EditText
            android:layout_width="81dp"
            android:layout_height="50dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/MinID" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.501"
            android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/textView" android:text="0"/>

</android.support.constraint.ConstraintLayout>